Futility_C_COMPILER_FLAGS=-lm -DFUTILITY_DBC -DFUTILITY_DBC -DUNIT_TEST -DDBL -g -O0

Futility_FORTRAN_FLAGS= -g -O0 -Wall -fcheck=all -fbacktrace
Futility_Fortran_COMPILER_FLAGS= -g -O0 -Wall -fcheck=all -fbacktrace

## Extra link flags (e.g., specification of fortran libraries)
Futility_EXTRA_LD_FLAGS=

## This is the command-line entry used for setting rpaths. In a build
## with static libraries it will be empty. 
Futility_SHARED_LIB_RPATH_COMMAND=
